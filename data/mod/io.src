
let io := cont {
    Print : func mesg, end {
        if end == null {
            end := "";
        }
        if isarr(mesg) {
            print("[");
            for i in mesg {
                print(i,",");
            }
            print("\b]");
        } else {
            print(mesg);
        }
        print(end);
    },
    
    Println : func mesg {
        Print(mesg,"\n");
    },

    Read : func {
        ret read;
    }
};
